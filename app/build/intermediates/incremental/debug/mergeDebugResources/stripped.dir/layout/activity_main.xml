<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/text_output3"
        android:layout_width="232dp"
        android:layout_height="32dp"
        android:layout_marginStart="36dp"
        android:text="Resultado"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="21sp"
        app:layout_constraintStart_toEndOf="@+id/text_format"
        app:layout_constraintTop_toBottomOf="@+id/text_format" />

    <TextView
        android:id="@+id/text_output2"
        android:layout_width="232dp"
        android:layout_height="31dp"
        android:layout_marginTop="67dp"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="68dp"
        android:text="Resultado"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="21sp"
        app:layout_constraintBottom_toBottomOf="@+id/text_output3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/text_format"
        app:layout_constraintTop_toTopOf="@+id/text_output1"
        app:layout_constraintVertical_bias="0.425" />

    <TextView
        android:id="@+id/text_output1"
        android:layout_width="230dp"
        android:layout_height="41dp"
        android:layout_marginStart="36dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="25dp"
        android:text="Resultado"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="21sp"
        app:layout_constraintBottom_toTopOf="@+id/text_format"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/text_format"
        app:layout_constraintTop_toBottomOf="@+id/imgFoto" />

    <TextView
        android:id="@+id/text_format"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="47dp"
        android:text="Output:"
        android:textColor="@color/black"
        android:textSize="21sp"
        app:layout_constraintBottom_toTopOf="@+id/text_output3"
        app:layout_constraintEnd_toStartOf="@+id/text_output2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_output1" />

    <FrameLayout
        android:id="@+id/imgFoto"
        android:layout_width="319dp"
        android:layout_height="405dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="55dp"
        app:layout_constraintBottom_toTopOf="@+id/text_output1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>